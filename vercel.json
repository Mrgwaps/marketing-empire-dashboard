{
  "version": 2,
  "framework": null,
  "buildCommand": null,
  "devCommand": null,
  "installCommand": null,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/login",
      "destination": "/public/login.html",
      "permanent": false
    },
    {
      "source": "/",
      "destination": "/public/index.html",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}